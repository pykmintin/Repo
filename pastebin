// Search EVERY element for "token" text
const allElements = document.querySelectorAll('*');
const matches = [];

allElements.forEach(el => {
  const text = el.textContent || '';
  if (text.toLowerCase().includes('token') || /\d+\s*\/\s*200000/.test(text)) {
    matches.push({
      element: el.tagName,
      text: text.trim().substring(0, 100), // First 100 chars
      visible: el.offsetParent !== null
    });
  }
});

console.log(`Found ${matches.length} matches:`);
matches.forEach((m, i) => {
  console.log(`${i + 1}. <${m.element}> ${m.text} ${m.visible ? '(VISIBLE)' : '(HIDDEN)'}`);
});
